public class Exercise_05_18_d {

	public static void main(String[] args) {

		for (int row = 1; row <= 6; row++) {

			for (int column = row; column > 1; column--)
				System.out.print("  ");

			for (int column = 1; column <= 6 + 1 - row; column++)
				System.out.print(column + " ");
			System.out.println();
		}
	}
}
